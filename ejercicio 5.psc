Proceso sucesorantecesor
	// Declaración
	Definir antecesor Como Entero;
	Definir num1 Como Entero;
	Definir sucesor Como Entero;
	// Proceso 
	Escribir 'Ingresa el número: ';
	Leer num1;
	antecesor <- num1-1;
	sucesor <- num1+1;
	Escribir 'El antecesor del número es: ', antecesor;
	Escribir 'El sucesor del número es: ', sucesor;
FinProceso
